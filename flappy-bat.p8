function _init()
	-- set variables

	position_x = 63
	position_y = 100

end

function _update()
	-- do calculations

 if (btn(➡️)) then
 
 	position_x += 1
 
 elseif (btn(⬅️)) then
 
 	position_x -= 1
 
 elseif (btn(⬆️)) then
 
 	position_y -= 1
 
 elseif (btn(⬇️)) then
 
 	position_y += 1
 
 end

end

function _draw()
	-- make visual stuff

	cls()
	map()
	
	spr(1, position_x, position_y)

end
