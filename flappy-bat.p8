__lua__
function _init()
	-- set variables

	px = 10
	py = 35
	gravity = 2
	
	flap = 0
	flap_down = 8
	
	-- funcion de cueva
	i_cave()

end

function _update()
	-- do calculations
	
	-- velocidad con la que cae 
	-- el personaje
	py += gravity
 
 if (btnp(⬆️)) then
 	flap = flap_down
 end
 
 if flap > 0 then
 
 	flap -= 1
 
 end
 
 py -= flap
 
 
 if py >= 120 then
 	py = 120
 elseif py <= 0 then
 	py = 0
 end

	u_cave()
	
end

function _draw()
	-- make visual stuff

	cls()
	map()
	
	spr(1, px, py)
	
	d_cave()

end


-->8
--cave
function i_cave()

	c1x = 100
	c1y = 40
	
	c2x = 100
	c2y = 80

end

function u_cave()

	c1x -= 1
	c2x -= 1



end

function d_cave()

	spr(5,c1x,c1y,2,2)
	spr(5,c2x,c2y,2,2)

end
